@model IEnumerable<MarioAuth.Models.MasterClass>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    <link rel="stylesheet" href="~/css/master.css" />
}
<div class="masterPage">
    <div class="intro">
        <div class="divPhoto">
            <img class="mainPhoto" src="~/img/master/masterPhoto.png">
        </div>
    </div>
    <table>   
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <div class="masterDate">
                        <h1> @Html.DisplayFor(modelItem => item.Date)</h1>
                    </div>
                </td>
                <td class="masterInfo">
                    <div class="masterTXT">
                        <h1>@Html.DisplayFor(modelItem => item.Title)</h1>
                        <p>@Html.DisplayFor(modelItem => item.Description)</p>
                    </div>
                </td>
            </tr>
        }
    </table>
